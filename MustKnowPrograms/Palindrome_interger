class Solution(object):
    def isPalindrome(self, x):
        """
        :type x: int
        :rtype: bool
        """
        # x = str(x)
        # rev = x[::-1]
        # return rev == x
        
        reverse, realval = 0, x
        
        while x >0:
            
            reverse = (reverse * 10) + (x % 10)
            x = int(x / 10)
            
            
        return realval == reverse
